<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="loginAnchorPane" prefHeight="300.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pl.n2god.mvc.LoginController">
    <children>
        <Button fx:id="loginButton" layoutX="173.0" layoutY="174.0" mnemonicParsing="false" text="Login" />
        <Label fx:id="primaryLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="126.0" layoutY="100.0" text="Podaj nazwę użytkownika:" textAlignment="CENTER" />
        <TextField fx:id="usernameTextField" alignment="CENTER_RIGHT" layoutX="121.0" layoutY="137.0" promptText="username" />
    </children>
</AnchorPane>
